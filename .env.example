# Soniox API Key
# Get your API key from https://console.soniox.com
SONIOX_API_KEY=your_api_key_here

# Vapi API Key (for creating assistants programmatically)
# Get from https://dashboard.vapi.ai
VAPI_API_KEY=your_vapi_api_key_here

# Vapi Assistant Configuration
# Assistant ID to update with Soniox transcriber
VAPI_ASSISTANT_ID=your_assistant_id_here
# Your transcriber WebSocket URL (from ngrok or your deployment)
VAPI_TRANSCRIBER_URL=wss://your-ngrok-url.ngrok.io/api/custom-transcriber

# Vapi Server Configuration (optional, defaults shown)
VAPI_SERVER_HOST=0.0.0.0
VAPI_SERVER_PORT=8080
